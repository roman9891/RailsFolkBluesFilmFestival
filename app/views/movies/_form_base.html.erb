<%if flash[:errors]%>
    <%=flash[:errors].each do |message|%>
        <%="ERROR: #{message}"%>
    <%end%>
<%end%>
<%=form_for @movie do |f|%>
    <%=f.label 'Name: '%>
    <%=f.text_field :title%>
    <%=f.label 'Link: '%>
    <%=f.text_field :url%>
    <%=f.collection_select :user_id, User.all, :id, :name%>
    <%=f.submit%>
<%end%>